<div class="page-header">
  <%= link_to submission_revisions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_submission_revision_path(@submission_revision), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show submission_revision</h1>
</div>

<dl class="dl-horizontal">
  <dt>Sid:</dt>
  <dd><%= @submission_revision.sid %></dd>

  <dt>Submission:</dt>
  <dd><%= @submission_revision.submission %></dd>

  <dt>Revision n:</dt>
  <dd><%= @submission_revision.revision_n %></dd>

  <dt>Aasm state:</dt>
  <dd><%= @submission_revision.aasm_state %></dd>

</dl>
